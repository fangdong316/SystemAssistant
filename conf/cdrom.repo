[Base]
name=Red Hat Enterprise Linux $releasever - $basearch - BASE
baseurl=file:///mnt/Server
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-redhat-release

[Cluster]
name=Red Hat Enterprise Linux $releasever Beta - $basearch - Cluster
